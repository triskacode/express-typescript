{
  "name": "express-typescript",
  "version": "1.0.0",
  "description": "Example express api with typescript",
  "main": "index.js",
  "scripts": {
    "start:dev": "DOTENV_CONFIG_PATH=.env.development ts-node-dev --respawn --transpile-only -r tsconfig-paths/register src/main",
    "start:prod": "DOTENV_CONFIG_PATH=.env.production node dist/main",
    "build": "tsc && tsc-alias",
    "test": "DOTENV_CONFIG_PATH=.env.test jest",
    "test:e2e": "DOTENV_CONFIG_PATH=.env.test jest --config test/jest-e2e.config.ts"
  },
  "keywords": [],
  "author": "triska.triskacode@gmail.com",
  "license": "ISC",
  "devDependencies": {
    "@types/cache-manager": "^4.0.2",
    "@types/compression": "^1.7.2",
    "@types/cors": "^2.8.12",
    "@types/express": "^4.17.14",
    "@types/jest": "^29.2.0",
    "@types/morgan": "^1.9.3",
    "@types/node": "^18.8.5",
    "@types/supertest": "^2.0.12",
    "jest": "^29.2.1",
    "supertest": "^6.3.0",
    "ts-jest": "^29.0.3",
    "ts-node-dev": "^2.0.0",
    "tsc-alias": "^1.7.0",
    "tsconfig-paths": "^4.1.0",
    "typescript": "^4.8.4"
  },
  "dependencies": {
    "cache-manager": "^4",
    "compression": "^1.7.4",
    "cors": "^2.8.5",
    "dotenv": "^16.0.3",
    "express": "^4.18.2",
    "joi": "^17.6.3",
    "morgan": "^1.10.0",
    "mysql2": "^2.3.3",
    "sequelize": "^6.25.1",
    "winston": "^3.8.2"
  }
}